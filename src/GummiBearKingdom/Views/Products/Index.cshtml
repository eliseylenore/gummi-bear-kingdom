@{
    ViewData["Title"] = "Gummi Bear Kingdom";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model IEnumerable<GummiBearKingdom.Models.Product>
<h2>Gummies!</h2>

@foreach (var product in Model)
{
    <h4>@Html.DisplayFor(modelProduct => product.Name) | @Html.ActionLink("Details", "Details", new { id = product.ProductId })</h4>
}



<p>@Html.ActionLink("Add new product", "Create", new { @class = "btn btn-success" })</p>